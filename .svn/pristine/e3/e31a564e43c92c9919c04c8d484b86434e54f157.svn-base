package com.yanbang.flow.entity;

/**
 * 流程跟踪信息<br>
 * 
 * 记录流程的执行情况
 * 
 * @author 徐春福
 * 
 */
public class FlowProcess implements java.io.Serializable {

	private static final long serialVersionUID = -3983950961045599117L;
	/**
	 * 跟踪编号
	 */
	private Integer processkey;
	/**
	 * 任务处理人
	 */
	private String processuser;
	/**
	 * 处理操作
	 */
	private String processoper;
	/**
	 * 处理意见
	 */
	private String processsuggestion;
	/**
	 * 处理日期时间
	 */
	private String processdatetime;
	/**
	 * 处理的任务节点
	 */
	private String processnodename;
	/**
	 * 处理的备注信息
	 */
	private String processremark;
	/**
	 * 流程编号
	 */
	private String flowid;

	/**
	 * 历史任务编号
	 */
	private Long historyTaskId;

	// Constructors

	/** default constructor */
	public FlowProcess() {
	}

	/** full constructor */
	public FlowProcess(String processuser, String processoper,
			String processsuggestion, String processdatetime,
			String processnodename, String processremark) {
		this.processuser = processuser;
		this.processoper = processoper;
		this.processsuggestion = processsuggestion;
		this.processdatetime = processdatetime;
		this.processnodename = processnodename;
		this.processremark = processremark;
	}

	// Property accessors

	public Integer getProcesskey() {
		return this.processkey;
	}

	public void setProcesskey(Integer processkey) {
		this.processkey = processkey;
	}

	public String getProcessuser() {
		return this.processuser;
	}

	public void setProcessuser(String processuser) {
		this.processuser = processuser;
	}

	public String getProcessoper() {
		return this.processoper;
	}

	public void setProcessoper(String processoper) {
		this.processoper = processoper;
	}

	public String getProcesssuggestion() {
		return this.processsuggestion;
	}

	public void setProcesssuggestion(String processsuggestion) {
		this.processsuggestion = processsuggestion;
	}

	public String getProcessdatetime() {
		return this.processdatetime;
	}

	public void setProcessdatetime(String processdatetime) {
		this.processdatetime = processdatetime;
	}

	public String getProcessnodename() {
		return this.processnodename;
	}

	public void setProcessnodename(String processnodename) {
		this.processnodename = processnodename;
	}

	public String getProcessremark() {
		return this.processremark;
	}

	public void setProcessremark(String processremark) {
		this.processremark = processremark;
	}

	public String getFlowid() {
		return flowid;
	}

	public void setFlowid(String flowid) {
		this.flowid = flowid;
	}

	public Long getHistoryTaskId() {
		return historyTaskId;
	}

	public void setHistoryTaskId(Long historyTaskId) {
		this.historyTaskId = historyTaskId;
	}

}